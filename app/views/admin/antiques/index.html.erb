<h1>Antiques</h1>

<table>
  <thead>
    <tr>
      <td><%= link_to "New Antique", new_admin_antique_path %></td>
    </tr>
  </thead>
  <tbody>
    <%- @antiques.each do |antique| -%>
      <tr>
        <td>
          <%= antique.sku %>
        </td>
        <td>
          <%= link_to antique.name, admin_antique_path(antique) %>
        </td>
        <td>
          <%= antique.description %>
        </td>
      </tr>
    <%- end -%>
    <tr>
  </tbody>
</table>
